<project name="target.compile.swc.mobile">
  
  <property file="${buildsupport.dir}/base/base-build.properties"/>
  <import file="${buildsupport.dir}/base/base-macros.xml"/>


  <target name="compile-swc-mobile">
    <copy-config config-name="${compile.config.name}"/>
    <compc-compile flex-dir-home="${flex.dir.home}" 
      src-path="${lib.dir.input}" 
      output-file="${lib.file.output}" 
      framework-config-name="airmobile" 
      compiler-args="${build.dir.output}/generated-compile-config.xml"/>
  </target>
  
</project>